                           <content>
                              <VBox fx:id="left_pane" spacing="10.0" />
                           </content>
                        </ScrollPane>
                     </content>
                  </TitledPane>
                <TitledPane animated="false" prefHeight="198.0" prefWidth="136.0" text="Others">
